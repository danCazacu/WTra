@prefix : <https://github.com/danCazacu/WTra#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix WTra: <https://github.com/danCazacu/WTra#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://github.com/danCazacu/WTra> .

<https://github.com/danCazacu/WTra> rdf:type owl:Ontology .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2002/07/owl#intersectionOf
owl:intersectionOf rdf:type rdfs:Datatype .


###  http://www.w3.org/2002/07/owl#unionOf
owl:unionOf rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://github.com/danCazacu/WTra#applicableTo
WTra:applicableTo rdf:type owl:ObjectProperty ;
                  rdfs:domain WTra:Traffic_Sign ;
                  rdfs:range WTra:Person .


###  https://github.com/danCazacu/WTra#hasBackgroundColor
WTra:hasBackgroundColor rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdfs:domain WTra:Traffic_Sign ;
                        rdfs:range WTra:Color .


###  https://github.com/danCazacu/WTra#hasBorderColor
WTra:hasBorderColor rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain WTra:Traffic_Sign ;
                    rdfs:range WTra:Color .


###  https://github.com/danCazacu/WTra#hasCode
WTra:hasCode rdf:type owl:ObjectProperty ;
             owl:inverseOf WTra:isCountry ;
             rdf:type owl:FunctionalProperty ,
                      owl:InverseFunctionalProperty ;
             rdfs:domain WTra:Country_Code ;
             rdfs:range WTra:Country .


###  https://github.com/danCazacu/WTra#hasForm
WTra:hasForm rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain WTra:Traffic_Sign ;
             rdfs:range WTra:Shape .


###  https://github.com/danCazacu/WTra#hasNotSign
WTra:hasNotSign rdf:type owl:ObjectProperty ;
                rdfs:domain WTra:Country ;
                rdfs:range WTra:Traffic_Sign .


###  https://github.com/danCazacu/WTra#hasSign
WTra:hasSign rdf:type owl:ObjectProperty ;
             rdfs:domain WTra:Country ;
             rdfs:range WTra:Traffic_Sign .


###  https://github.com/danCazacu/WTra#isCountry
WTra:isCountry rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdf:type owl:FunctionalProperty ,
                        owl:InverseFunctionalProperty ;
               rdfs:domain WTra:Country_Code ;
               rdfs:range WTra:Country .


#################################################################
#    Data properties
#################################################################

###  https://github.com/danCazacu/WTra#hasImageLink
WTra:hasImageLink rdf:type owl:DatatypeProperty ;
                  rdfs:domain WTra:Traffic_Sign ;
                  rdfs:range xsd:string .


###  https://github.com/danCazacu/WTra#hasLegalRegulations
WTra:hasLegalRegulations rdf:type owl:DatatypeProperty ;
                         rdfs:domain WTra:Traffic_Sign ;
                         rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/2002/07/owl#Thing
owl:Thing rdfs:subClassOf owl:Thing .


###  https://github.com/danCazacu/WTra#Color
WTra:Color rdf:type owl:Class ;
           rdfs:subClassOf owl:Thing .


###  https://github.com/danCazacu/WTra#Country
WTra:Country rdf:type owl:Class ;
             rdfs:subClassOf owl:Thing .


###  https://github.com/danCazacu/WTra#Country_Code
WTra:Country_Code rdf:type owl:Class ;
                  rdfs:subClassOf owl:Thing .


###  https://github.com/danCazacu/WTra#Information_Sign
WTra:Information_Sign rdf:type owl:Class ;
                      rdfs:subClassOf WTra:Traffic_Sign ;
                      rdfs:comment "The information signs give you more information about the start (and end) of a kind of traffic situation." ;
                      rdfs:label "Information Sign" .


###  https://github.com/danCazacu/WTra#Mandatory_Sign
WTra:Mandatory_Sign rdf:type owl:Class ;
                    rdfs:subClassOf WTra:Traffic_Sign ;
                    rdfs:comment "The mandatory signs impose an obligation/command that road users must comply with." ;
                    rdfs:label "Mandatory Sign" .


###  https://github.com/danCazacu/WTra#Person
WTra:Person rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing .


###  https://github.com/danCazacu/WTra#Priority_Sign
WTra:Priority_Sign rdf:type owl:Class ;
                   rdfs:subClassOf WTra:Traffic_Sign ;
                   rdfs:comment "The priority signs have an influence on the priority rules. They indicate who has priority and who should give priority." ;
                   rdfs:label "Priority Sign" .


###  https://github.com/danCazacu/WTra#Prohibitory_Sign
WTra:Prohibitory_Sign rdf:type owl:Class ;
                      rdfs:subClassOf WTra:Traffic_Sign ;
                      rdfs:comment "The prohibition signs forbid a certain action. They indicate certain actions that road users are not allowed to do." ;
                      rdfs:label "Prohibitory Sign" .


###  https://github.com/danCazacu/WTra#Road_Sign
WTra:Road_Sign rdf:type owl:Class ;
               owl:equivalentClass WTra:Traffic_Sign ;
               rdfs:subClassOf WTra:Signs .


###  https://github.com/danCazacu/WTra#Safety_Sign
WTra:Safety_Sign rdf:type owl:Class ;
                 rdfs:subClassOf WTra:Signs ;
                 rdfs:comment "The safety signs are often used in companies or factories to provide information about safety." .


###  https://github.com/danCazacu/WTra#Shape
WTra:Shape rdf:type owl:Class ;
           rdfs:subClassOf owl:Thing .


###  https://github.com/danCazacu/WTra#Signs
WTra:Signs rdf:type owl:Class ;
           rdfs:subClassOf owl:Thing .


###  https://github.com/danCazacu/WTra#Traffic_Sign
WTra:Traffic_Sign rdf:type owl:Class ;
                  rdfs:subClassOf WTra:Signs ;
                  rdfs:comment "The traffic signs are located on the side or top of the road. They give directions on how we should behave on the road, so that the traffic can proceed safely and smoothly. Everyone must know the traffic signs!" .


###  https://github.com/danCazacu/WTra#Warning_Sign
WTra:Warning_Sign rdf:type owl:Class ;
                  rdfs:subClassOf WTra:Traffic_Sign ;
                  rdfs:comment "The warning signs are placed to warn road users of a potentially dangerous traffic situation." ;
                  rdfs:label "Warning Sign" .


#################################################################
#    Individuals
#################################################################

###  https://github.com/danCazacu/WTra#begin_of_a_priority_road
WTra:begin_of_a_priority_road rdf:type owl:NamedIndividual ,
                                       WTra:Information_Sign ,
                                       WTra:Priority_Sign ;
                              WTra:applicableTo WTra:driver ;
                              WTra:hasBackgroundColor WTra:yellow ;
                              WTra:hasBorderColor WTra:white ;
                              WTra:hasForm WTra:diamond ;
                              WTra:hasImageLink "https://traffic-rules.com/img/europe/ro/signs/information/information-priority-road.png" ;
                              WTra:hasLegalRegulations "Priority road (priority at all following intersections up to an end-of-priority-road, yield or stop sign). In some countries, the meaning was changed to the next intersection only." ;
                              rdfs:comment "This traffic sign indicates the begin of a priority road. You have priority at all following crossroads until the end of the priority road." .


###  https://github.com/danCazacu/WTra#black
WTra:black rdf:type owl:NamedIndividual ,
                    WTra:Color .


###  https://github.com/danCazacu/WTra#blue
WTra:blue rdf:type owl:NamedIndividual ,
                   WTra:Color .


###  https://github.com/danCazacu/WTra#circle
WTra:circle rdf:type owl:NamedIndividual ,
                     WTra:Shape .


###  https://github.com/danCazacu/WTra#crossing_for_pedestrians
WTra:crossing_for_pedestrians rdf:type owl:NamedIndividual ,
                                       WTra:Information_Sign ;
                              WTra:applicableTo WTra:driver ,
                                                WTra:pedestrian ;
                              WTra:hasBackgroundColor WTra:blue ;
                              WTra:hasBorderColor WTra:white ;
                              WTra:hasForm WTra:square ;
                              WTra:hasImageLink "https://traffic-rules.com/img/europe/gr/signs/information/information-crossing-pedestrian.png" ;
                              WTra:hasLegalRegulations """Pedestrian crossings allow people to cross the road safely. These crossings are usually marked by white stripes on the road and are also known as zebra crossings.

Some pedestrian crossings have zig zag lines marked on the road before the crossing. Take extra care when you see a zig zag line on the road as this means you are approaching a crossing which you may not be able to see because of a curve, crest or dip in the road.

Do not overtake any vehicle that is stopping or has stopped at a crossing.""" ;
                              rdfs:comment "This traffic sign indicates a crossing for pedestrians." .


